{
 
  "name": "Profile Launcher",
  "description": "Launch Quick Profiles of Social Media ",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": { 
	"128": "icon_128.png" 
	},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "Display.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}